{
  "name": "form-after-all",
  "version": "0.0.1-alpha-1",
  "description": "reactive vue form",
  "author": "erraX",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erraX/form-after-all.git"
  },
  "keywords": [
    "vue",
    "form",
    "composition-api"
  ],
  "bugs": {
    "url": "https://github.com/erraX/form-after-all/issues"
  },
  "homepage": "https://github.com/erraX/form-after-all#readme",
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "prebuild": "rimraf es lib",
    "check-ts": "tsc",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "clean": "rimraf -rf es lib",
    "build": "rollup -c",
    "build:watch": "rollup -cw",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn check-ts"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx,vue}": [
      "prettier --parser=typescript --write",
      "git add"
    ]
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-beta.1",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.10.4",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.14",
    "@vue/composition-api": "^1.0.0-beta.1",
    "babel-jest": "^26.1.0",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "husky": "^4.2.5",
    "rollup": "^2.19.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "typescript": "^3.9.6",
    "vue": "^2.6.11",
    "vue-jest": "^3.0.6",
    "vuepress": "^1.5.2"
  },
  "dependencies": {
    "lodash-es": "^4.17.15"
  }
}
